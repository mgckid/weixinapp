page{padding-bottom: 3.3rem;background-color: #fff;}
.brtop {
    border-top: .8rem solid #EBEBEB;
}
.detail_count .h3 {
    color: #FF7043;
    line-height: 1.3rem;
    font-size: 28rpx;
}
.detail_count .detail_down {
    line-height: 1.2rem;
    font-size: 26rpx;
}
.detail_count .detail_down .span {
    color: #FF7043;
    padding: 0 .2rem;
}

.detail_count {
    padding: .3rem .8rem;
    border-bottom: 1px solid #D5D5D5;
    background: #FFFFFF url(http://www.tm.hxqc.com/wxapp/maintain/detail_down.png) no-repeat 95% center/1.4rem;
}
.detail_count.on {
    background: #FFFFFF url(http://www.tm.hxqc.com/wxapp/maintain/detail_up.png) no-repeat 95% center/1.4rem;
}
.cart_item{overflow: hidden;}
.cart_name{background-color: #ebebeb;}
.child_name{width: 60%;float: left;line-height:1.3rem;}

.coupon_use {
    padding: .5rem .8rem;
    border-bottom: 1px solid #D5D5D5;
    background: url(http://www.tm.hxqc.com/wxapp/maintain/icon-rjt.png) no-repeat 96% center/.5rem;
    color: #737373;
    clear: both;overflow: hidden;
}
.coupon_use .coupon_left {
    float: left;
    margin-right: .2rem;
    font-size: 32rpx;
}
.coupon_use .use_can {
    float: left;
    background-color: #fc7442;
    color: #fff;
    font-size: 24rpx;
    line-height: 1.2rem;
    margin-top: .05rem;
    padding: .1rem .15rem;
}
.coupon_use .coupon_right {
    float: right;
    font-size: 30rpx;
    padding-right: 1rem;
}
.sideslip{right: -85%;
    width: 85%;
    z-index: 9998;
    transition: all 300ms;
    position: fixed;
    top: 0;
    bottom: 0px;
    box-shadow: rgba(0, 0, 0, 0.2) -4px 0px 5px 2px;
    opacity: 1;
    box-sizing: border-box;
    transform: translateX(0%);
    background-color: rgb(255, 255, 255);
}
.sideslip.on{transform: translateX(-100%);}

.couList {
    padding: 0 .5rem;
}
.detLists .li {
    margin-top: 1rem;
    float: left;
    width: 100%;
    background: #FFFFFF;
}
.detLists .coupon {
    background-color: #FFFFFF;
    box-shadow: 0 .05rem .25rem 0 rgba(0,0,0,.3);
    -webkit-box-shadow: 0 .05rem .25rem 0 rgba(0,0,0,.3);
    clear: both;
    overflow: hidden;
    height: 4.4rem;
}
.detLists .coupon.coupon_check {
    background: url(http://www.tm.hxqc.com/wxapp/maintain/coupon_check.png) no-repeat right top/1.4rem;
}
.detLists .coupon .cona {
    display: inline;
    float: left;
    position: relative;
}
.detLists .coupon .cona image {
    width: 4.8rem;
    height: 4.4rem;
}
.detLists .coupon .cona .cona-text {
    clear: both;
    line-height: 1.5rem;
    position: absolute;
    top: .5rem;
    color: #fff;
    text-align: center;
    width: 96%;
}
.detLists .coupon .cona .cona-text .em {
    font-size: 30rpx;
}
.detLists .coupon .cona .cona-text .span {
    font-size: 38rpx;
    display: inline-block;
    text-align: center;
}
.detLists .coupon .p {
    display: inline-block;
    padding-left: .5rem;
    white-space: nowrap;
    font-size: 24rpx;
    color: #a2a2a2;
    height: 1rem;
    line-height: 1.4rem;
}
.detLists .coupon .cona .p.use_range {
    line-height: 1.1rem;
    text-align: center;
    position: absolute;
    left: 0;
    bottom: 1.4rem;
    color: #C0E7FF;
    overflow: inherit;
    text-overflow: inherit;
    white-space: inherit;
    width: 4.6rem;
    padding-left: 0;
}
.detLists .coupon .conText {
    padding-top: .3rem;
    /*width: 72%;*/
    width:calc(100% - 5rem)
}
.detLists .coupon .p .span.use_left {
    float: left;
    white-space: nowrap;
    overflow: hidden;
}
.detLists .coupon .p .span.use_right {
    display: block;
    margin-left: 2.6rem;
    line-height: 1.1rem;
    margin-top: .15rem;
    white-space: initial;
}
.box_notuse {
    padding: 0 .5rem;
}
.box_notuse .coupon_btn {
    display: inline-block;
    text-align: center;
    width: 94%;
    font-size: 28rpx;
    line-height: 1.9rem;
    margin: .825rem 0;
    background: #FFFFFF;
    color: #41AAEC;
    border: 1px solid #D5D5D5;
    border-radius: 0.2rem;
}
.detail_list{display: none;}
.detail_list.on{display: block;}