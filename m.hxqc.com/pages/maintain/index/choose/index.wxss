/*保养项目选择*/
.item_change{position: relative; border-bottom: .8rem solid #EBEBEB;background-color: #fff; padding: 0 .68rem; height: 3.5rem; line-height: .75rem; border-bottom: .8rem solid #EBEBEB; font-size: 30rpx;}
.item_change .car_left{width: 65%;height: 100%;border-right: 1px solid #CCCCCC;float: left;}
.item_change .car_left image.brand_icon{float: left; width:1.5rem;height: 1.5rem; margin-left: .3rem; margin-top: .9rem;}
.item_change .car_left .p.brand_name{margin-top: .7rem;}
.item_change .car_left .p.brand_name span.name_left{float:left;width: 5rem;overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.item_change .car_left .p.brand_name a.name_right{float:left;width: 1.5rem;color:#FF7146;font-size: 26rpx;text-decoration: underline;}
.item_change .car_left .p.drive_length{color: #999999; font-size: 24rpx; margin-top: .5rem;}
.item_change .car_left .p{color: #333333; float: left; width: 80%; margin-left: .38rem; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.item_change .car_left .p .name_left{margin-right: .2rem;}
.item_change .car_left .p navigator{color:red;display: inline-block;float: right;text-decoration: underline;margin-right: .2rem;}
.item_change .car_right{width: 34%;height: 100%;float: left;}
.item_change .car_right .p{margin-left: .8rem;float: left;}
.item_change .car_right .p.drive_mile{margin-top: .2rem;font-size: 28rpx;}
.item_change .car_right .p.drive_mile .mile_left{float: left;width:3rem;border: none;}
.item_change .car_right .p.drive_mile .mile_right{float: left;line-height: 1.4rem;}
.item_change .car_right .p.drive_text{color: #999999; font-size: 28rpx; margin-top: .3rem;}
.item_advice{ padding: .4rem .68rem; line-height: 1.2rem; font-size: 26rpx; color: #FF7043;}
.keep_list{padding:.38rem .68rem;}
.keep_list .keep_title{font-size: 28rpx;margin-bottom: .4rem;line-height: 1rem;color:#333333;}
.keep_list .keep_cell{clear:both;overflow:hidden;position:relative;border:1px solid #CCCCCC;text-align: left;padding:.4rem;margin-bottom: .38rem;background-color: #FFFFFF;border-radius: 3px;}
.keep_list .keep_cell.select{border:1px solid #FF7043;}
.keep_list .advice_item.select .choose{position: absolute;top: 0;right: 0;width: 1.6rem;height: 2.2rem;background-image: url("http://www.tm.hxqc.com/wxapp/maintain/item_check.jpg");background-size: 1.6rem 2.2rem;background-repeat: no-repeat;}
.keep_list .own_choose.select .choose{position: absolute;top: 0;right: 0;width: 1.6rem;height: 2.2rem;background-image: url("http://www.tm.hxqc.com/wxapp/maintain/item_check.jpg");background-size: 1.6rem 2.2rem;background-repeat: no-repeat;}
.keep_list .keep_cell .item_tip{float:left;margin-top:.15rem;margin-left:.5rem;width: .8rem;height: .8rem;background-image: url("http://www.tm.hxqc.com/wxapp/maintain/item_tip.png");background-size: .8rem;background-repeat: no-repeat;}
.keep_list .keep_cell.dislabled{background-color: #eeeeee;color: #bcbcbc;}
.keep_list .keep_cell:nth-of-type(3n+3){margin-right: 0;}
.keep_list .keep_cell images{width: 1rem;height: .9rem;}
.keep_list .keep_cell .p{line-height: 1.4rem;height:1.4rem;max-width:80%;overflow:hidden;font-size: 24rpx;color:#999999;float: left;}
.payment_status {position: fixed;left: 0;bottom: 0;background-color: #fff;padding: .2rem .8rem;width: 100%;box-sizing: border-box;box-shadow: 0 -3px 3px rgba(0,0,0,.2);z-index: 200;}
.check_price_btn{margin: .2rem 0;line-height: 2.2rem;text-align: center;background-color: #ff7143;color: #fff;font-size: 30rpx;display: block;box-shadow: 0 .05rem .25rem 0 rgba(0, 0, 0, .3);-webkit-box-shadow: 0 .05rem .25rem 0 rgba(0, 0, 0, .3);}
page{padding-bottom: 2.6rem;}
.mask{position: fixed;width: 100%;height: 100%;z-index: 12345;top: 0;left: 0;background-color: rgba(0,0,0,.6);display: none;}
.mask.on{display: block;}
.keep_popup {position: fixed;width: 100%;height: 100%;z-index: 12346;top: 4.42rem;left: 0;display: none;}
.keep_popup.on{display: block;}
.popup_top {position: relative;}
.popup_top image {width: 100%;max-height: 7rem;}
.popup_top .popup_cancel {position: absolute;top: .43rem;right: .43rem;width: .9rem;height: 1rem;background-image: url("http://www.tm.hxqc.com/wxapp/maintain/popup_cancel.png");background-repeat: no-repeat;background-size: .9rem;}
.popup_top .top_text {position: absolute;bottom: 0;line-height: 1.7rem;height: 1.7rem;width: 100%;background-color: rgba(0, 0, 0, .6);text-align: center;color: #FFFFFF;font-size: 32rpx;}
.popup_bottom {padding: 1.5rem .8rem;background-color: #FAFAFA;}
.STYLE5 {color: #FF6633;font-weight: bold;font-size: 14px;margin-bottom:0.6rem;display: block;}
.popup_bottom .p {font-size: 28rpx;line-height: 1rem;color: #9E9E9E;margin-bottom: .3rem;}