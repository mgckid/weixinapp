.clearline{position:absolute; border-radius:5px; transition:all .2s ease;}
.clearline .num{position:absolute; width:18px; height:18px; font-size:12px; background-color:#787878; color:#fff !important; text-align:center; line-height:18px; border-radius:50%; cursor:pointer; transition:all .2s ease; padding:0;}
.clearline .dot{position:absolute; width:6px; height:6px; background:#787878; border-radius:50%; transition:all .2s ease;}
.clearline .etip{position:absolute; width:auto; min-width:90px;top:10px; left:-117px;text-align: left; display: none;}
.clearline .etip span{display:inline-block; width:100%; padding:7px;background:#ed4851;color:#fff !important; text-shadow:none; z-index:100; font-size:120%}
.clearline .etip i{position:absolute; width:9px; height:5px;top:-5px;right:auto;left:20px;background:url(../images/ctrob-air.png) no-repeat; display:none; }
.clearline-type1{border-right:2px solid #787878; border-top:2px solid #787878;}
.clearline-type1 .num{top:-9px; left:-9px;}
.clearline-type1 .dot{right:-4px; bottom:-4px;padding:0;}
.clearline-type1 .etip{left:27px; text-align: left;}
.clearline-type2{ border-left:2px solid #787878; border-top:2px solid #787878;}
.clearline-type2 .num{top:-9px; right:-9px;}
.clearline-type2 .dot{left:-4px; bottom:-4px;padding:0;}
.clearline-type3{ border-left:2px solid #787878; border-bottom:2px solid #787878;}
.clearline-type3 .num{bottom:-9px; right:-9px;}
.clearline-type3 .dot{left:-4px; top:-4px;padding:0;}
.clearline-type3 .etip{text-align: left; top:-20px;}
.clearline-type4{ border-right:2px solid #787878;border-bottom:2px solid #787878; width:0;}
