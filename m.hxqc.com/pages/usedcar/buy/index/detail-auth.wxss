.clearline-type4 .dot{right:-4px; top:-4px; padding:0;}
.clearline-type4 .etip{left:27px; top:-23px; text-align: left;}
.clearline.on{border-color:#ed4851; z-index: 11;}
.clearline.on .num{background-color:#ed4851;}
