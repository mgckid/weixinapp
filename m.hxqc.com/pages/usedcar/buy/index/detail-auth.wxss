.car-servise .servise-title .servise-type {float: left;line-height: 1.5rem;color: #e02c36;font-weight: bold;font-size: 32rpx;margin-left: 1.5rem;}
.car-servise .uspecial{clear: both;overflow:hidden;padding: .7rem .86rem;}
.car-servise .uspecial>.buyBtn{float: left;width: 310rpx;height: 2.5rem;border: 2px solid #e02c36;font-size: 32rpx;color: #e02c36;line-height: 2.5rem;text-align:center;}
.car-servise .uspecial>.buyBtn:nth-of-type(1){margin-right: 1.1rem;}
