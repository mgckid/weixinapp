.content{padding-bottom: 1.88rem;}
.clearfix:after{content: '\200B'; display: block; clear: both; height: 0; overflow: hidden; visibility: hidden;}
.clearfix{zoom: 1;}
/*头部图片*/
.ucar-pic{clear: both;overflow:hidden;position: relative;}
.ucar-pic .pic{width: 100%;}
.ucar-pic .picIcon{position: absolute;right: .5rem;bottom: .8rem;width: 4rem;}
.ucar-pic .number{position: absolute;left:0;bottom:1.3rem;padding-left:.65rem;padding-right:.3rem;height: 1.8rem;line-height: 1.8rem;font-size: 32rpx;color: #fff;background-color:rgba(0,0,0,.6);}
.ucar-pic .text{position: absolute;right:.7rem;bottom:.9rem;width: 3rem;height: 3rem;line-height:3rem;text-align:center;border-radius: 50%;background-color:rgba(0,0,0,.65);font-size: 32rpx;color: #fff;}
/*车辆详细信息*/
.ucar-detail{padding: 0 .72rem;border-bottom: 1px solid #d5d5d5;}
.ucar-detail .title{font-size: 32rpx;color: #222;font-weight: bold;line-height: 1.3rem;margin-top: .4rem;}
.ucar-detail .price{font-size: 32rpx;color: #f60000;font-weight: bold;line-height: 1.8rem;}
