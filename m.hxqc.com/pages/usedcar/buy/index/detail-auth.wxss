.ucar-detail .text{clear: both;overflow:hidden;}
.ucar-detail .detail{margin-top: .35rem;}
.ucar-detail .detail span{float: left;width: 50%;line-height: 1.8rem;font-size: 32rpx;color: #666;}
.parameter{clear: both;overflow:hidden;padding: .4rem 0;}
.parameter span{float: left;width:50%;font-size: 30rpx;color: #999;line-height:1.8rem;}
/*特色服务*/
.car-servise{border-bottom: 1px solid #d5d5d5;box-shadow: 0 2px 2px rgba(0,0,0,.1);clear: both;}
.car-servise .servise-title{clear: both;overflow: hidden;padding: .68rem;background-color: #f5f5f5;}
.car-servise .servise-title.borderbt{border-bottom: 1px solid #d5d5d5;}
