<!-- 预约看车弹窗 -->
<view class="ubespeak-mask {{switch_show}}" bindtap="onClickClose"></view>
<view class="ubespeak-cover {{switch_show}}" id="ubespeak-cover">
  <form style="margin-bottom:1rem;" bindsubmit="onSubmit">
    <view class="tips">填写手机、预约时间，我们的客服将尽快与您联系</view>
    <view class="inputbox">
      <input type="hidden" placeholder="车源编号" name="carSourceId" value="{{detailInfo.car_source_no}}" style="display:none" />
      <image class="img" src="http://www.tm.hxqc.com/wxapp/usedcar/inputbox1.jpg" alt=""></image>
      <input type="number" placeholder="请输入手机号" name="mobile" focus="{{focusMobile}}" maxlength="11" id="mobile_no" bindblur="mobileBlur" />
      <span class="push_clear"></span> 
      <span id="getmCode" class="sms_verify" data-sendtype="10" bindtap="onSendVerify">发送验证码</span> 
    </view>
    <view class="inputbox" id="verify">
      <image class="img" src="http://www.tm.hxqc.com/wxapp/usedcar/inputbox3.png" alt=""></image>
      <input type="text" placeholder="验证码" name="captcha" style="width: 50%;" maxlength="6" />
    </view>
    <view class="inputbox">
      <image class="img" src="http://www.tm.hxqc.com/wxapp/usedcar/inputbox2.jpg" alt=""></image>

      <!--<input type="text" placeholder="预约时间" name="look_datetime" class="yy_time" id="beginTime1" onfocus="look_datetime.blur()"/>     -->
      <picker mode="date" value="{{time}}" start="2017-01-01" end="2017-09-01" bindchange="bindTimeChange" style="line-height:1.9rem;font-size:32rpx;color:#989898;">
        <view class="yy_time">
          <block wx:if='{{time == null}}'>预约时间</block>
          <block wx:else>{{time}}</block>
        </view>
      </picker>
      <span class="push_clear"></span> 
    </view>
    <button type="button" form-type="submit" class="submit" id="buycar" style="margin-bottom: .2rem;">预约</button>
    <view class="yu_verify">收不到短信？需要
      <text class="voice_code" data-sendtype="20" bindtap="onSendVerify">语音验证码</text><span id="send_again"></span>  </view>
  </form>
</view>

<!--检测异常弹窗-->
<view class="td_tips_show {{tip_no}}">{{tip}}</view>
<view class="td_tips_bg {{tip_no}}" bindtap="onClickHide"></view>

<!--底部按钮-->
<view class="ufooter">
  <block wx:if="{{detailButtonInfo.carDetail.detail.car_on_sale == 1}}">
    <view class="ubespeak" bindtap="onClickChoose">预约看车</view>
  </block>
  <block wx:else>
    <view class="ubespeak" style="background-color: #AAAAAA">{{detailButtonInfo.carDetail.detail.car_on_sale_name}}</view>
  </block>
  <view class="call" catchtap='onCall' data-tel="400-186-8555">客服电话</view>
</view>