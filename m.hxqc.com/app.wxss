view{font-family: "微软雅黑";font-size: 28rpx}
/*车型颜色*/
.detail-color{float: left;width: 66%;margin-top: -.45rem;}
.detail-color .span{float:left;width: 1.2rem;height: 1.2rem;margin-right: .55rem;margin-top: .45rem;box-shadow: .1rem .1rem .1rem rgba(0,0,0,.1);}
/*蒙版*/
.sideslipMask{transition: all 300ms;opacity: 0;z-index: -1; position: fixed; width: 100%; height: 100%; top: 0px; left: 0px;  background-color: rgba(0, 0, 0, 0.6);}
.sideslipMask.on{opacity: 1; z-index: 9997;}
.sidesFixed{transition: all 300ms;transform: translateX(0%);-webkit-transform:translateX(0%);opacity: 0;z-index: -1;right: -80%;width: 80%;position: fixed;top: 0rem;bottom: 0px;box-shadow: rgba(0, 0, 0, 0.2) -4px 0px 5px 2px;box-sizing: border-box;
  background-color: rgb(255, 255, 255);}
.sidesFixed.on{transform:translateX(-100%);-webkit-transform:translateX(-100%);opacity: 1;z-index: 9998;}
/*页面loading*/
.pageLoading {display:block; opacity: 1;z-index:99999999;position: fixed;top: 0;left: 0;width: 100%;height: 100%;z-index: 99999;background: #fff;}
.pageLoading.hide{opacity: 0;z-index: -1;display:none;}
.pageLoading .spinner {position: absolute;top: 50%;left: 50%;width: 68px;height: 68px;text-align: center;margin: -34px 0 0 -34px;background: -webkit-gradient(radial, 100% 50%, 0, 100% -38%, 100, from(#bebebe), to(rgba(255, 255, 255, 0)));border-radius: 100%;-webkit-animation: rotate .8s linear 0s infinite;animation: rotate .8s linear 0s infinite;}
.pageLoading .spinner .spinner-inner {position: relative;top: 3px;left: 3px;width: 62px;height: 62px;background: #fff;border-radius: 100%;}
.pageLoading .spinner .cutout {position: absolute;top: 34px;left: 35px;border-right: 34px solid #fff;border-bottom: 34px solid #fff;-webkit-border-top-left-radius: 0px;-webkit-border-top-right-radius: 0px;-webkit-border-bottom-right-radius: 34px;-webkit-border-bottom-left-radius: 0px;width: 0em;height: 0em;padding: 0;}
.pageLoading .spinner .s-circle {width: 3px;height: 3px;background: #bebebe;position: absolute;top: 33px;right: 0;border-radius: 3px;}
.pageLoading .loading-logo {position: absolute;top: 50%;left: 50%;margin-left: -25px;margin-top: -25px;color: #1DBD8D;width: 50px;height: 50px;line-height: 50px;}
.pageLoading .loading-logo image{width: 50px;height: 50px;}
@-webkit-keyframes
rotate {0% {-webkit-transform:rotate(0)}100% {-webkit-transform:rotate(360deg)}}
