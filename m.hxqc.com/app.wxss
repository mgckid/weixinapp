.body{width: 100%;margin:0px;float: left;font-size: 0.8rem;font-family: 微软雅黑;}
.container {height: 100%;display: flex;flex-direction: column;align-items: center;justify-content: space-between;padding: 200rpx 0;box-sizing: border-box;} 
.wrap{font-family: Arial, Helvetica, sans-serif;padding:1rem;font-size:0.8rem;}
image.defaultImg{background: url(http://www.hxqc.com/wukocar/img/default/nopic_160_160.jpg) no-repeat 50% 50%/cover;}
/** 修改默认的navigator点击态 **/
.navigator-hover {color:red;}
/** 自定义其他点击态样式类 **/
